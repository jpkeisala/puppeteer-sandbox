{
  "name": "pptr",
  "version": "1.0.0",
  "description": "Automated Software Testing With Puppeteer",
  "main": "index.js",
  "scripts": {
    "test": "node  ./node_modules/mocha/bin/mocha --timeout=30000 ./tests",
